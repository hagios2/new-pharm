 <b-card-text> {{ element.name }} </b-card-text>\r\n                    <template v-slot:footer>\r\n                      <small class=\"text-muted\">{{ index }}</small>\r\n                    </template>\r\n                  </b-card>\r\n                </div>\r\n              </transition-group>\r\n            </draggable>\r\n          </b-card>\r\n        </draggable>\r\n      </div>\r\n      <!-- scrumboard-modal  -->\r\n      <b-modal id=\"modal-center\" centered title=\"Draggable 1\" hide-footer>\r\n        <b-form>\r\n          <b-form-group\r\n            id=\"input-group-1\"\r\n            label=\"Title:\"\r\n            label-for=\"input-1\"\r\n            class=\"mb-3\"\r\n          >\r\n            <b-form-input\r\n              id=\"input-1\"\r\n              type=\"text\"\r\n              required\r\n              placeholder=\"Enter email\"\r\n            ></b-form-input>\r\n          </b-form-group>\r\n\r\n          <b-form-group>\r\n            <b-form-textarea\r\n              id=\"textarea\"\r\n              placeholder=\"Description\"\r\n              rows=\"5\"\r\n              max-rows=\"9\"\r\n            ></b-form-textarea>\r\n          </b-form-group>\r\n\r\n          <b-form-group id=\"input-group-3\" label=\"Food:\" label-for=\"input-3\">\r\n            <b-form-select\r\n              id=\"input-3\"\r\n              options=\"foods\"\r\n              required\r\n            ></b-form-select>\r\n          </b-form-group>\r\n\r\n          <b-form-group id=\"input-group-4\">\r\n            <b-form-checkbox-group id=\"checkboxes-4\">\r\n              <b-form-checkbox value=\"me\">Check me out</b-form-checkbox>\r\n              <b-form-checkbox value=\"that\">Check that out</b-form-checkbox>\r\n            </b-form-checkbox-group>\r\n          </b-form-group>\r\n\r\n          <b-button type=\"submit\" variant=\"primary\">Submit</b-button>\r\n          <b-button type=\"reset\" variant=\"danger\">Reset</b-button>\r\n        </b-form>\r\n      </b-modal>\r\n\r\n      <div class=\"col-3\">\r\n        <draggable>\r\n          <b-card title=\"Draggable 2\">\r\n            <draggable\r\n              class=\"list-group\"\r\n              :list=\"list2\"\r\n              group=\"people\"\r\n              @change=\"log\"\r\n              v-bind=\"dragOptions\"\r\n              @start=\"drag = true\"\r\n              @end=\"drag = false\"\r\n            >\r\n              <transition-group\r\n                tag=\"p\"\r\n                type=\"transition\"\r\n                :name=\"!drag ? 'flip-list' : null\"\r\n              >\r\n                <div\r\n                  class=\"\"\r\n                  v-for=\"(element, index) in list2\"\r\n                  :key=\"element.name\"\r\n                >\r\n                  <b-card\r\n                    class=\"mb-30 cursor-pointer\"\r\n                    :img-src=\"element.img\"\r\n                    img-top\r\n                  >\r\n                    <b-card-text> {{ element.name }} </b-card-text>\r\n                    <template v-slot:footer>\r\n                      <small class=\"text-muted\">{{ index }}</small>\r\n                    </template>\r\n                  </b-card>\r\n                </div>\r\n              </transition-group>\r\n            </draggable>\r\n          </b-card>\r\n        </draggable>\r\n      </div>\r\n      <b-col md=\"3\">\r\n        <b-card class=\"cursor-pointer\">\r\n          <div class=\"card-title mb-0\">\r\n            <b-button\r\n              @click=\"isShow = !isShow\"\r\n              v-if=\"isShow\"\r\n              tag=\"div\"\r\n              class=\"ul-scrum-btn d-flex align-items-center border-0\"\r\n            >\r\n              <i class=\"i-Add text-20 mr-2\"></i>\r\n              <p class=\"m-0 text-muted\">Add Project List</p>\r\n            </b-button>\r\n\r\n            <b-input-group v-else class=\"mt-3\">\r\n              <b-input-group-append>\r\n                <b-button\r\n                  @click=\"addProjectList\"\r\n                  :disabled=\"projectList == ''\"\r\n                  variant=\"primary\"\r\n                  size=\"sm\"\r\n                  >Add</b-button\r\n                >\r\n              </b-input-group-append>\r\n              <b-form-input\r\n                class=\"ul-scrumboard-form-input\"\r\n                type=\"text\"\r\n                v-model=\"projectList\"\r\n              ></b-form-input>\r\n              <b-input-group-append>\r\n                <b-button @click=\"isShow = !isShow\" variant=\"primary\" size=\"sm\"\r\n                  >X</b-button\r\n                >\r\n              </b-input-group-append>\r\n            </b-input-group>\r\n          </div>\r\n        </b-card>\r\n      </b-col>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport draggable from \"vuedraggable\";\r\n\r\nexport default {\r\n  name: \"two-lists\",\r\n  display: \"Two Lists\",\r\n\r\n  order: 1,\r\n  components: {\r\n    draggable,\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      isShow: true,\r\n      drag: false,\r\n      projectList: \"\",\r\n      list1: [\r\n        {\r\n          name: \"John\",\r\n          id: 1,\r\n          img: require(\"@/assets/images/products/headphone-1.jpg\"),\r\n        },\r\n        {\r\n          name: \"Joao\",\r\n          id: 2,\r\n          img: require(\"@/assets/images/products/iphone-1.jpg\"),\r\n        },\r\n        {\r\n          name: \"Jean\",\r\n          id: 3,\r\n          img: require(\"@/assets/images/products/speaker-1.jpg\"),\r\n        },\r\n        {\r\n          name: \"Gerard\",\r\n          id: 4,\r\n          img: require(\"@/assets/images/products/watch-1.jpg\"),\r\n        },\r\n      ],\r\n      list2: [\r\n        { name: \"Juan\", id: 5 },\r\n        { name: \"Edgard\", id: 6 },\r\n        { name: \"Johnson\", id: 7 },\r\n      ],\r\n    };\r\n  },\r\n  computed: {\r\n    dragOptions() {\r\n      return {\r\n        animation: 200,\r\n        group: \"description\",\r\n        disabled: false,\r\n      };\r\n    },\r\n  },\r\n  methods: {\r\n    add: function() {\r\n      this.list.push({ name: \"Juan\" });\r\n    },\r\n    replace: function() {\r\n      this.list = [{ name: \"Edgard\" }];\r\n    },\r\n    clone: function(el) {\r\n      return {\r\n        name: el.name + \" cloned\",\r\n      };\r\n    },\r\n    log: function(evt) {\r\n      // window.console.log(evt);\r\n    },\r\n    btnStatus() {\r\n      if (this.projectList != \"\") {\r\n        this.isBtnDisable = false;\r\n      }\r\n    },\r\n    addProjectList() {},\r\n  },\r\n};\r\n</script>\r\n<style>\r\n.button {\r\n  margin-top: 35px;\r\n}\r\n.flip-list-move {\r\n  transition: transform 0.5s;\r\n}\r\n.no-move {\r\n  transition: transform 0s;\r\n}\r\n.ghost {\r\n  opacity: 0.5;\r\n  background: #c8ebfb;\r\n}\r\n.list-group {\r\n  min-height: 20px;\r\n}\r\n.list-group-item {\r\n  cursor: move;\r\n}\r\n.list-group-item i {\r\n  cursor: pointer;\r\n}\r\n.ul-scrum-btn:focus {\r\n  box-shadow: none;\r\n  /* box-shadow: 0 0 0 0.2rem rgba(82, 73, 90, 0.5); */\r\n}\r\n.ul-scrum-btn:hover {\r\n  box-shadow: none;\r\n}\r\n.ul-scrumboard-form-input:focus {\r\n  box-shadow: none;\r\n}\r\n/* The heart of the matter */\r\n</style>\r\n"]}]}