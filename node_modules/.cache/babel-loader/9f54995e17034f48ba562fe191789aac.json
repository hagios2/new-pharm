ata","boardTitle","addBoard","title","cardList","comment","selectedListId","enabled","isShow","isCard","drag","projectList","computed","mapGetters","dragOptions","animation","group","disabled","methods","mapActions","onSubmit","toggleCard","addProjectList","editCardListPage","back"],"sourceRoot":"src/views/apps/scrumboard","sources":["editScrumBoard.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <breadcumb :page=\"'Add Board'\" :folder=\"'Apps'\" />\r\n\r\n    <div class=\"mb-30\">\r\n      <b-card>\r\n        <div class=\"d-flex justify-content-between align-items-center\">\r\n          <b-button @click=\"back\" variant=\"outline-primary\" size=\"sm\"\r\n            >Back</b-button\r\n          >\r\n\r\n          <p class=\"m-0 card-title\">{{ getEditBoardList.title }}</p>\r\n          <b-button variant=\"outline-danger\" size=\"sm\">Delete </b-button>\r\n        </div>\r\n      </b-card>\r\n    </div>\r\n\r\n    <div class=\"editScrumBoardContainer d-flex mb-30\">\r\n      <!-- <b-col md=\"12\">\r\n        <pre>\r\n          {{ getEdit }}\r\n        </pre>\r\n        </b-col> -->\r\n      <div\r\n        class=\"col-md-3 ul-board-item\"\r\n        v-for=\"(div, key) in getEditBoardList.list\"\r\n        :key=\"key\"\r\n      >\r\n        <b-card :title=\"div.title\">\r\n          <draggable\r\n            class=\"list-group\"\r\n            group=\"people\"\r\n            v-bind=\"dragOptions\"\r\n            @start=\"drag = true\"\r\n            @end=\"drag = false\"\r\n          >\r\n            <transition-group\r\n              type=\"transition\"\r\n              :name=\"!drag ? 'flip-list' : null\"\r\n            >\r\n              <!-- <div class v-for=\"(element, index) in list1\" :key=\"element.name\"> -->\r\n              <div v-for=\"list in div.cardList\" :key=\"list.id\">\r\n                <b-card\r\n                  class=\"mb-30 cursor-pointer\"\r\n                  :img-src=\"list.coverImage\"\r\n                  img-top\r\n                  v-b-modal.modal-center\r\n                  @click=\"editCardListPage(list)\"\r\n                >\r\n                  <b-card-text>{{ list.title }}</b-card-text>\r\n                  <template v-slot:footer>\r\n                    <small class=\"text-muted\"></small>\r\n                  </template>\r\n                </b-card>\r\n              </div>\r\n              <!-- </div> -->\r\n            </transition-group>\r\n          </draggable>\r\n\r\n          <!-- toggleCard  -->\r\n          <template v-slot:footer>\r\n            <b-button\r\n              @click=\"toggleCard(key)\"\r\n              v-if=\"selectedListId != key\"\r\n              block\r\n              variant=\"primary\"\r\n              >Add Card</b-button\r\n            >\r\n            <b-input-group v-else-if=\"selectedListId === key\" class=\"mt-3\">\r\n              <b-input-group-append>\r\n                <b-button\r\n                  :disabled=\"addBoard.cardList == ''\"\r\n                  variant=\"primary\"\r\n                  size=\"sm\"\r\n                  @click=\"\r\n                    addCard({ list: div, cardListTitle: addBoard.cardList })\r\n                  \"\r\n                  >Add</b-button\r\n                >\r\n              </b-input-group-append>\r\n              <b-form-input\r\n                v-model=\"addBoard.cardList\"\r\n                class=\"ul-scrumboard-form-input\"\r\n                type=\"text\"\r\n                v-on:keyup.enter=\"\r\n                  addCard({ list: div, cardListTitle: addBoard.cardList })\r\n                \"\r\n              ></b-form-input>\r\n              <b-input-group-append>\r\n                <b-button @click=\"toggleCard\" variant=\"primary\" size=\"sm\"\r\n                  >X</b-button\r\n                >\r\n              </b-input-group-append>\r\n            </b-input-group>\r\n          </template>\r\n        </b-card>\r\n      </div>\r\n\r\n      <!-- scrumboard-modal  -->\r\n      <b-modal\r\n        id=\"modal-center\"\r\n        size=\"lg\"\r\n        centered\r\n        title=\"Draggable 1\"\r\n        hide-footer\r\n      >\r\n        <b-form>\r\n          <b-form-group id=\"input-group-1\" label-for=\"input-1\" class=\"mb-4\">\r\n            <b-form-input\r\n              id=\"input-1\"\r\n              type=\"text\"\r\n              required\r\n              v-model=\"getEditCardList.title\"\r\n              placeholder=\"title\"\r\n            ></b-form-input>\r\n          </b-form-group>\r\n\r\n          <b-form-group class=\"mb-4\">\r\n            <b-form-textarea\r\n              id=\"textarea\"\r\n              v-model=\"getEditCardList.description\"\r\n              rows=\"5\"\r\n              placeholder=\"Description\"\r\n            ></b-form-textarea>\r\n          </b-form-group>\r\n\r\n          <b-form-group class=\"mb-4\">\r\n            <b-form-input\r\n              v-model=\"addBoard.comment\"\r\n              @keyup.enter=\"addComment(addBoard.comment)\"\r\n              id=\"input-1\"\r\n              type=\"text\"\r\n              placeholder=\"Write Comments....\"\r\n            ></b-form-input>\r\n          </b-form-group>\r\n\r\n          <b-list-group class=\"mb-4\">\r\n            <div\r\n              class=\"d-flex w-100 align-items-baseline\"\r\n              v-for=\"comment in getEditCardList.comments\"\r\n              :key=\"comment.id\"\r\n            >\r\n              <p class=\"ul-widget4__img todo-img\">\r\n                <img :src=\"comment.image\" class=\"rounded-circle\" />\r\n              </p>\r\n              <b-list-group-item class=\"mb-4 w-100\">{{\r\n                comment.text\r\n              }}</b-list-group-item>\r\n            </div>\r\n          </b-list-group>\r\n\r\n          <b-button\r\n            @click.prevent=\"onSubmit(getEditCardList)\"\r\n            type=\"button\"\r\n            variant=\"primary\"\r\n            >Submit</b-button\r\n          >\r\n\r\n          <b-button type=\"reset\" variant=\"danger\">Reset</b-button>\r\n        </b-form>\r\n      </b-modal>\r\n\r\n      <b-col md=\"3\">\r\n        <b-card class=\"cursor-pointer\">\r\n          <div class=\"card-title mb-0\">\r\n            <b-button\r\n              @click=\"isShow = !isShow\"\r\n              v-if=\"isShow\"\r\n              tag=\"div\"\r\n              class=\"ul-scrum-btn d-flex align-items-center border-0\"\r\n            >\r\n              <i class=\"i-Add text-20 mr-2\"></i>\r\n              <p class=\"m-0 text-muted\">Add Project List</p>\r\n            </b-button>\r\n\r\n            <b-input-group v-else class=\"mt-3\">\r\n              <b-input-group-append>\r\n                <b-button\r\n                  @click=\"addList(addBoard.title)\"\r\n                  :disabled=\"addBoard.title == ''\"\r\n                  variant=\"primary\"\r\n                  size=\"sm\"\r\n                  >Add</b-button\r\n                >\r\n              </b-input-group-append>\r\n              <b-form-input\r\n                class=\"ul-scrumboard-form-input\"\r\n                type=\"text\"\r\n                v-model=\"addBoard.title\"\r\n                v-on:keyup.enter=\"addList(addBoard.title)\"\r\n              ></b-form-input>\r\n              <b-input-group-append>\r\n                <b-button @click=\"isShow = !isShow\" variant=\"primary\" size=\"sm\"\r\n                  >X</b-button\r\n                >\r\n              </b-input-group-append>\r\n            </b-input-group>\r\n          </div>\r\n        </b-card>\r\n      </b-col>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport draggable from \"vuedraggable\";\r\nimport { mapGetters, mapActions } from \"vuex\";\r\n\r\nexport default {\r\n  name: \"two-lists\",\r\n  display: \"Two Lists\",\r\n\r\n  order: 1,\r\n  components: {\r\n    draggable,\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      boardTitle: \"untitle\",\r\n      addBoard: {\r\n        title: \"\",\r\n        cardList: \"\",\r\n        comment: \"\",\r\n      },\r\n      selectedListId: null,\r\n      enabled: true,\r\n      isShow: true,\r\n      isCard: false,\r\n      drag: false,\r\n      projectList: \"\",\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters([\r\n      \"getBoardList\",\r\n      \"getEditBoardList\",\r\n      \"getEditCardList\",\r\n      \"getList\",\r\n    ]),\r\n    dragOptions() {\r\n      return {\r\n        animation: 200,\r\n        group: \"description\",\r\n        disabled: false,\r\n      };\r\n    },\r\n  },\r\n  methods: {\r\n    ...mapActions([\r\n      \"editCardList\",\r\n      \"addList\",\r\n      \"addCard\",\r\n      \"addComment\",\r\n      \"updateCardList\",\r\n      \"updateBoard\",\r\n    ]),\r\n    onSubmit(data) {\r\n      // console.log(\"testing\");\r\n      this.updateCardList(data);\r\n    },\r\n    toggleCard(data) {\r\n      this.selectedListId = data;\r\n    },\r\n    addProjectList() {},\r\n    editCardListPage(data) {\r\n      this.editCardList(data);\r\n    },\r\n    back() {\r\n      this.$router.push(\"/app/apps/scrumboard\");\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style>\r\n.button {\r\n  margin-top: 35px;\r\n}\r\n.flip-list-move {\r\n  transition: transform 0.5s;\r\n}\r\n.no-move {\r\n  transition: transform 0s;\r\n}\r\n.ghost {\r\n  opacity: 0.5;\r\n  background: #c8ebfb;\r\n}\r\n.list-group {\r\n  min-height: 20px;\r\n}\r\n.list-group-item {\r\n  cursor: move;\r\n}\r\n.list-group-item i {\r\n  cursor: pointer;\r\n}\r\n.ul-scrum-btn:focus {\r\n  box-shadow: none;\r\n  /* box-shadow: 0 0 0 0.2rem rgba(82, 73, 90, 0.5); */\r\n}\r\n.ul-scrum-btn:hover {\r\n  box-shadow: none;\r\n}\r\n.ul-scrumboard-form-input:focus {\r\n  box-shadow: none;\r\n}\r\n.editScrumBoardContainer {\r\n  overflow-x: scroll;\r\n}\r\n.ul-board-item {\r\n  min-width: 400px;\r\n}\r\n</style>\r\n"]}]}