animate","value","variant","methods","rendomValue","mounted","beforeDestroy","clearInterval"],"sourceRoot":"src/views/app/ui-kits","sources":["progressbar.vue"],"sourcesContent":["<template>\r\n  <div class=\"main-content\">\r\n    <breadcumb :page=\"'Progress Bar'\" :folder=\"'UI Kits'\" />\r\n\r\n    <b-row class=\" mb-30\">\r\n      <!-- left-side -->\r\n      <b-col md=\"6\">\r\n        <b-row>\r\n          <!-- start::basic-progressbar -->\r\n          <b-col md=\"12\" class=\" mb-30\">\r\n            <b-card title=\"Basic Progress Bar\">\r\n              <h5>Default width</h5>\r\n              <b-progress :value=\"value\" class=\"mb-3\"></b-progress>\r\n\r\n              <h5>Custom widths</h5>\r\n              <b-progress :value=\"value\" class=\"w-75 mb-2\"></b-progress>\r\n              <b-progress :value=\"value\" class=\"w-50 mb-2\"></b-progress>\r\n              <b-progress :value=\"value\" class=\"w-25\"></b-progress>\r\n            </b-card>\r\n          </b-col>\r\n          <b-col md=\"12\" class=\" mb-30\">\r\n            <b-card title=\"Basic Progress Bar With diff. Background\">\r\n              <b-progress\r\n                variant=\"success\"\r\n                :value=\"value\"\r\n                class=\"mb-3\"\r\n              ></b-progress>\r\n              <b-progress\r\n                variant=\"primary\"\r\n                :value=\"value\"\r\n                class=\"w-75 mb-2\"\r\n              ></b-progress>\r\n              <b-progress\r\n                variant=\"danger\"\r\n                :value=\"value\"\r\n                class=\"w-50 mb-2\"\r\n              ></b-progress>\r\n              <b-progress\r\n                variant=\"warning\"\r\n                :value=\"value\"\r\n                class=\"w-25\"\r\n              ></b-progress>\r\n            </b-card>\r\n          </b-col>\r\n\r\n          <b-col md=\"12\" class=\" mb-30\">\r\n            <b-card title=\"Custom progress label\">\r\n              <h6>Custom label via default slot</h6>\r\n              <b-progress :max=\"max\" height=\"2rem\">\r\n                <b-progress-bar variant=\"danger\" :value=\"value\">\r\n                  Progress: <strong>{{ value.toFixed(2) }} / {{ max }}</strong>\r\n                </b-progress-bar>\r\n              </b-progress>\r\n\r\n              <h6 class=\"mt-3\">Custom label via property</h6>\r\n              <b-progress :max=\"max\">\r\n                <b-progress-bar\r\n                  variant=\"success\"\r\n                  :value=\"value\"\r\n                  :label=\"`${((value / max) * 100).toFixed(2)}%`\"\r\n                ></b-progress-bar>\r\n              </b-progress>\r\n\r\n              <h6 class=\"mt-3\">Custom label via property (HTML support)</h6>\r\n              <b-progress :max=\"max\">\r\n                <b-progress-bar\r\n                  :value=\"value\"\r\n                  :label-html=\"`<del>${value}</del>`\"\r\n                ></b-progress-bar>\r\n              </b-progress>\r\n            </b-card>\r\n          </b-col>\r\n        </b-row>\r\n\r\n        <b-row>\r\n          <b-col md=\"12\" class=\" mb-30\">\r\n            <b-card title=\"Progres Bar Animated\">\r\n              <b-progress\r\n                :value=\"changeValue\"\r\n                :max=\"max\"\r\n                show-progress\r\n                animated\r\n              ></b-progress>\r\n              <b-progress class=\"mt-2\" :max=\"changeMax\" show-value>\r\n                <b-progress-bar\r\n                  :value=\"changeValue * (6 / 10)\"\r\n                  variant=\"success\"\r\n                ></b-progress-bar>\r\n                <b-progress-bar\r\n                  :value=\"changeValue * (2.5 / 10)\"\r\n                  variant=\"warning\"\r\n                ></b-progress-bar>\r\n                <b-progress-bar\r\n                  :value=\"changeValue * (1.5 / 10)\"\r\n                  variant=\"danger\"\r\n                ></b-progress-bar>\r\n              </b-progress>\r\n\r\n              <b-button class=\"mt-3\" @click=\"rendomValue\">Click me</b-button>\r\n            </b-card>\r\n          </b-col>\r\n        </b-row>\r\n\r\n        <b-row>\r\n          <b-col md=\"12\" class=\" mb-30\">\r\n            <b-card title=\"Animated backgrounds\">\r\n             \r\n\r\n              <b-progress\r\n                :value=\"animatedValue\"\r\n                variant=\"success\"\r\n                striped\r\n                :animated=\"animate\"\r\n              ></b-progress>\r\n              <b-progress\r\n                :value=\"animatedValue\"\r\n                variant=\"info\"\r\n                striped\r\n                :animated=\"animate\"\r\n                class=\"mt-2\"\r\n              ></b-progress>\r\n              <b-progress\r\n                :value=\"animatedValue\"\r\n                variant=\"warning\"\r\n                striped\r\n                :animated=\"animate\"\r\n                class=\"mt-2\"\r\n              ></b-progress>\r\n              <b-progress\r\n                :value=\"animatedValue\"\r\n                variant=\"danger\"\r\n                :animated=\"animate\"\r\n                class=\"mt-3\"\r\n              ></b-progress>\r\n\r\n              <b-button\r\n                variant=\"secondary\"\r\n                @click=\"animate = !animate\"\r\n                class=\"mt-3\"\r\n              >\r\n                {{ animate ? \"Stop\" : \"Start\" }} Animation\r\n              </b-button>\r\n            </b-card>\r\n          </b-col>\r\n        </b-row>\r\n      </b-col>\r\n\r\n      <!-- right-side -->\r\n      <b-col md=\"6\">\r\n        <b-row>\r\n          <b-col md=\"12\" class=\" mb-30\">\r\n            <b-card title=\"Basic Progress Bar With Labels\">\r\n              \r\n\r\n              <h6>No label</h6>\r\n              <b-progress :value=\"value\" :max=\"max\" class=\"mb-3\"></b-progress>\r\n\r\n              <h6>Value label</h6>\r\n              <b-progress\r\n                :value=\"value\"\r\n                :max=\"max\"\r\n                show-value\r\n                class=\"mb-3\"\r\n              ></b-progress>\r\n\r\n              <h6>Progress label</h6>\r\n              <b-progress\r\n                :value=\"value\"\r\n                :max=\"max\"\r\n                show-progress\r\n                class=\"mb-3\"\r\n              ></b-progress>\r\n\r\n              <h6>Value label with precision</h6>\r\n              <b-progress\r\n                :value=\"value\"\r\n                :max=\"max\"\r\n                :precision=\"2\"\r\n                show-value\r\n                class=\"mb-3\"\r\n              ></b-progress>\r\n\r\n              <h6>Progress label with precision</h6>\r\n              <b-progress\r\n                :value=\"value\"\r\n                :max=\"max\"\r\n                :precision=\"2\"\r\n                show-progress\r\n                class=\"mb-3\"\r\n              ></b-progress>\r\n            </b-card>\r\n          </b-col>\r\n\r\n          <b-col md=\"12\" class=\" mb-30\">\r\n            <b-card title=\"Solid background variants\">\r\n              <div v-for=\"(bar, key) in bars\" :key=\"key\" class=\"row mb-1\">\r\n                <div class=\"col-sm-3\">{{ bar.variant }}:</div>\r\n                <div class=\"col-sm-9 pt-1\">\r\n                  <b-progress\r\n                    :value=\"bar.value\"\r\n                    :variant=\"bar.variant\"\r\n                    :key=\"bar.variant\"\r\n                  ></b-progress>\r\n                </div>\r\n              </div>\r\n            </b-card>\r\n          </b-col>\r\n\r\n          <b-col md=\"12\" class=\" mb-30\">\r\n            <b-card title=\"Multiple bars\">\r\n              <b-progress :max=\"multipleMax\" class=\"mb-3\">\r\n                <b-progress-bar\r\n                  variant=\"primary\"\r\n                  :value=\"multipleValues[0]\"\r\n                ></b-progress-bar>\r\n                <b-progress-bar\r\n                  variant=\"danger\"\r\n                  :value=\"multipleValues[1]\"\r\n                ></b-progress-bar>\r\n                <b-progress-bar\r\n                  variant=\"warning\"\r\n                  :value=\"multipleValues[2]\"\r\n                ></b-progress-bar>\r\n              </b-progress>\r\n\r\n              <b-progress show-progress :max=\"multipleMax\" class=\"mb-3\">\r\n                <b-progress-bar\r\n                  variant=\"danger\"\r\n                  :value=\"multipleValues[0]\"\r\n                ></b-progress-bar>\r\n                <b-progress-bar\r\n                  variant=\"success\"\r\n                  :value=\"multipleValues[1]\"\r\n                ></b-progress-bar>\r\n                <b-progress-bar\r\n                  variant=\"dark\"\r\n                  :value=\"multipleValues[2]\"\r\n                ></b-progress-bar>\r\n              </b-progress>\r\n\r\n              <b-progress show-value striped :max=\"multipleMax\" class=\"mb-3\">\r\n                <b-progress-bar\r\n                  variant=\"info\"\r\n                  :value=\"multipleValues[0]\"\r\n                ></b-progress-bar>\r\n                <b-progress-bar\r\n                  variant=\"success\"\r\n                  :value=\"multipleValues[1]\"\r\n                ></b-progress-bar>\r\n                <b-progress-bar\r\n                  variant=\"danger\"\r\n                  :value=\"multipleValues[2]\"\r\n                ></b-progress-bar>\r\n              </b-progress>\r\n\r\n              <b-progress :max=\"multipleMax\">\r\n                <b-progress-bar\r\n                  variant=\"primary\"\r\n                  :value=\"multipleValues[0]\"\r\n                  show-progress\r\n                ></b-progress-bar>\r\n                <b-progress-bar\r\n                  variant=\"warning\"\r\n                  :value=\"multipleValues[1]\"\r\n                  animated\r\n                  show-progress\r\n                ></b-progress-bar>\r\n                <b-progress-bar\r\n                  variant=\"info\"\r\n                  :value=\"multipleValues[2]\"\r\n                  striped\r\n                  show-progress\r\n                ></b-progress-bar>\r\n              </b-progress>\r\n            </b-card>\r\n          </b-col>\r\n          <b-col md=\"12\" class=\" mb-30\">\r\n            <b-card title=\"Progressbar Height\">\r\n            \r\n              <div>\r\n                <h5>Default height</h5>\r\n                <b-progress\r\n                  :value=\"value\"\r\n                  show-progress\r\n                  class=\"mb-3\"\r\n                ></b-progress>\r\n\r\n                <h5>Custom heights</h5>\r\n                <b-progress\r\n                  variant=\"success\"\r\n                  striped\r\n                  height=\"2rem\"\r\n                  :value=\"value\"\r\n                  show-progress\r\n                  class=\"mb-2\"\r\n                ></b-progress>\r\n                <b-progress\r\n                  variant=\"danger\"\r\n                  animated\r\n                  height=\"20px\"\r\n                  :value=\"value\"\r\n                  show-progress\r\n                  class=\"mb-2\"\r\n                ></b-progress>\r\n                <b-progress\r\n                  variant=\"warning\"\r\n                  height=\"2px\"\r\n                  :value=\"value\"\r\n                ></b-progress>\r\n              </div>\r\n            </b-card>\r\n          </b-col>\r\n        </b-row>\r\n      </b-col>\r\n    </b-row>\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  metaInfo: {\r\n    // if no subcomponents specify a metaInfo.title, this title will be used\r\n    title: \"ProgressBar\"\r\n  },\r\n  data() {\r\n    return {\r\n      animatedValue: 75,\r\n      //   animated-background\r\n      animate: true,\r\n      //   basic-progressbar value\r\n      value: 75,\r\n\r\n      // custom-progress-label\r\n      value: 33.333333333,\r\n      max: 50,\r\n\r\n      // solid-background-variants\r\n      key: 1,\r\n      bars: [\r\n        { variant: \"success\", value: 75 },\r\n        { variant: \"info\", value: 75 },\r\n        { variant: \"warning\", value: 75 },\r\n        { variant: \"danger\", value: 75 },\r\n        { variant: \"primary\", value: 75 },\r\n        { variant: \"secondary\", value: 75 },\r\n        { variant: \"dark\", value: 75 }\r\n      ],\r\n      timer: null,\r\n\r\n      // progressbar animated\r\n      changeValue: 45,\r\n      changeMax: 100,\r\n\r\n      // multiple-bar\r\n      multipleValues: [15, 30, 20],\r\n      multipleMax: 100\r\n    };\r\n  },\r\n  methods: {\r\n    rendomValue() {\r\n      this.changeValue = Math.random() * this.max;\r\n    }\r\n  },\r\n  mounted() {\r\n    this.timer = setInterval(() => {\r\n      this.bars.forEach(bar => (bar.value = 25 + Math.random() * 75));\r\n    }, 2000);\r\n  },\r\n  beforeDestroy() {\r\n    clearInterval(this.timer);\r\n    this.timer = null;\r\n  }\r\n};\r\n</script>"]}]}